    details = <
        ["de"] = <
            language = <[ISO_639-1::de]>
            purpose = <"*To record anticipatory decisions about the overall intent of care and possible interventions, usually asserted by an authorised clinician. (en)">
            keywords = <"*DNR (en)", "*DNAR (en)", "*DNACPR (en)", "*NFR (en)", "*resuscitation (en)", "*resuscitate (en)", "*EoL (en)", "*end of life (en)", "*directive (en)", "*preference (en)", "*goal (en)", "*care (en)", "*intervention (en)", "*ceiling (en)", "*limit (en)", "*limitation (en)", "*treatment (en)", "*scope (en)", "*escalation (en)", "*intent (en)", "*EOLC (en)", "*life-prolonging (en)", "*life-saving (en)", "*palliative (en)">
            use = <"*Use to record anticipatory decisions about the overall intent of care and possible interventions (including treatments or activities) for an individual. The decisions will support clinicians by providing guidance regarding possible treatments or activities that may be life-saving, life-prolonging or cause undesirable side effects. 
    
    Each decision should be made in response to the individual's overall health and circumstances, and usually asserted by an authorised clinician after consideration of the individual's advance care directives or stated preferences.
    
    Ideally, the directives are established as part of integrated care or end-of-life planning, and not as a reaction to an acute or emergency situation.
    
    It may be necessary to record temporary overrides to usual advance intervention decisions in specific circumstances, for example during pregnancy or preoperatively. In this situation it will be necessary to record a separate instance of this archetype, using a combination of relevant validity and review dates. (en)">
            misuse = <"Nicht zur Aufzeichnung einer 'Patientenverfügung' oder der Präferenzen der Person für die Pflege verwenden - verwenden Sie zu diesem Zweck EVALUATION.advance_care_directive.">
        >
        ["nb"] = <
            language = <[ISO_639-1::nb]>
            purpose = <"For å registrere en forhåndsbeslutning om den overordnede intensjonen med omsorg/behandling, samt hensiktsmessig behandling eller intervensjoner for et individ. Denne bestemmelsen er vanligvis tatt av en eller flere klinikere som er autorisert til det.">
            keywords = <"HLR-minus, R minus, resucitering, DNR, Do not resucitate, EoL, end of life, direktiv, behandlingsmål, intervensjon, beslutning, livsforlengende, palliativ, gjenoppliving, forhåndssamtale, forberedende samtale", ...>
            use = <"For å registrere en forhåndsbeslutning om den overordnede intensjonen med omsorg/behandling, samt hensiktsmessige intervensjoner (inkludert behandling og tiltak) for et individ. Beslutningene vil støtte klinikere ved å fungere som retningslinjer knyttet til mulige behandlinger eller tiltak som kan være livreddende, livsforlengende eller føre til uønskede konsekvenser for individet.
    
    Hver beslutning bør være tilpasset individets overordnede helse og situasjon. Beslutningene blir vanligvis tatt av en eller flere klinikere som er autorisert til det, etter også å ha tatt hensyn til individets og/eller pårørendes ønsker i form av livstestamente eller andre oppgitte preferanser.
    
    Ideelt skal beslutningene være en integrert del av behandlingsplan eller plan for omsorg i livets sluttfase, og ikke gjort under tidspress i en akutt eller nødsituasjon.
    
    Det kan være nødvendig å gjøre midlertidige avvik fra beslutningene i spesielle tilfeller, for eksempel ved svangerskap eller preoperativt. Da vil det være nødvendig å lage separate instanser av denne arketypen, og sette de relevante gyldighetsdatoene i Protokoll-delen.">
            misuse = <"Skal ikke brukes for å registrere individets livstestament eller andre personlige ønsker, bruk i stedet arketypen \"Fremtidig behandlingsvalg\" (EVALUATION.advance_care_directive).">
        >
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record anticipatory decisions about the overall intent of care and possible interventions, usually asserted by an authorised clinician.">
            keywords = <"DNR", "DNAR", "DNACPR", "NFR", "resuscitation", "resuscitate", "EoL", "end of life", "directive", "preference", "goal", "care", "intervention", "ceiling", "limit", "limitation", "treatment", "scope", "escalation", "intent", "EOLC", "life-prolonging", "life-saving", "palliative">
            use = <"Use to record anticipatory decisions about the overall intent of care and possible interventions (including treatments or activities) for an individual. The decisions will support clinicians by providing guidance regarding possible treatments or activities that may be life-saving, life-prolonging or cause undesirable side effects. 
    
    Each decision should be made in response to the individual's overall health and circumstances, and usually asserted by an authorised clinician after consideration of the individual's advance care directives or stated preferences.
    
    Ideally, the directives are established as part of integrated care or end-of-life planning, and not as a reaction to an acute or emergency situation.
    
    It may be necessary to record temporary overrides to usual advance intervention decisions in specific circumstances, for example during pregnancy or preoperatively. In this situation it will be necessary to record a separate instance of this archetype, using a combination of relevant validity and review dates.">
            misuse = <"Not to be used to record an 'Advance care directive' or the individual's preferences for care - use EVALUATION.advance_care_directive for this purpose.">
            copyright = <"© openEHR Foundation">
        >
    >

definition
    EVALUATION[id1] matches {    -- Advance intervention decisions
        data matches {
            ITEM_TREE[id2] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id3] occurrences matches {0..1} matches {    -- Intent of care
                        value matches {
                            DV_TEXT[id9003] 
                        }
                    }
                    ELEMENT[id4] occurrences matches {0..1} matches {    -- Rationale
                        value matches {
                            DV_TEXT[id9004] 
                        }
                    }
                    ELEMENT[id5] occurrences matches {0..1} matches {    -- CPR decision
                        value matches {
                            DV_TEXT[id9005] 
                            DV_CODED_TEXT[id9006] matches {
                                defining_code matches {[ac9000]}    -- CPR decision (synthesised)
                            }
                        }
                    }
                    ELEMENT[id10] occurrences matches {0..1} matches {    -- Description
                        value matches {
                            DV_TEXT[id9007] 
                        }
                    }
                    CLUSTER[id11] matches {    -- Per intervention
                        items cardinality matches {1..*; unordered} matches {
                            ELEMENT[id12] matches {    -- Intervention
                                value matches {
                                    DV_CODED_TEXT[id9008] matches {
                                        defining_code matches {[ac9001]}    -- Intervention (synthesised)
                                    }
                                    DV_TEXT[id9009] 
                                }
                            }
                            ELEMENT[id25] occurrences matches {0..1} matches {    -- Decision
                                value matches {
                                    DV_CODED_TEXT[id9010] matches {
                                        defining_code matches {[ac9002]}    -- Decision (synthesised)
                                    }
                                }
                            }
                            ELEMENT[id30] occurrences matches {0..1} matches {    -- Conditions
                                value matches {
                                    DV_TEXT[id9011] 
                                }
                            }
                            ELEMENT[id31] occurrences matches {0..1} matches {    -- Comment
                                value matches {
                                    DV_TEXT[id9012] 
                                }
                            }
                        }
                    }
                    allow_archetype CLUSTER[id32] matches {     -- Additional details
                        include
                            archetype_id/value matches {/.*/}
                    }
                    ELEMENT[id33] occurrences matches {0..1} matches {    -- Patient awareness
                        value matches {
                            DV_TEXT[id9013] 
                        }
                    }
                    ELEMENT[id34] occurrences matches {0..1} matches {    -- Carer awareness
                        value matches {
                            DV_TEXT[id9014] 
                        }
                    }
                    ELEMENT[id36] occurrences matches {0..1} matches {    -- Overall comment
                        value matches {
                            DV_TEXT[id9015] 
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id37] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id38] occurrences matches {0..1} matches {    -- Last updated
                        value matches {
                            DV_DATE_TIME[id9016] 
                        }
                    }
                    ELEMENT[id39] occurrences matches {0..1} matches {    -- Valid period start
                        value matches {
                            DV_DATE_TIME[id9017] 
                        }
                    }
                    ELEMENT[id40] occurrences matches {0..1} matches {    -- Valid period end
                        value matches {
                            DV_DATE_TIME[id9018] 
                        }
                    }
                    ELEMENT[id41] occurrences matches {0..1} matches {    -- Review due
                        value matches {
                            DV_DATE_TIME[id9019] 
                        }
                    }
                    ELEMENT[id42] occurrences matches {0..1} matches {    -- Mandate
                        value matches {
                            DV_TEXT[id9020] 
                            DV_EHR_URI[id9021] 
                        }
                    }
                    allow_archetype CLUSTER[id35] matches {     -- Digital representation
                        include
                            archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia_source\.v0\..*/}
                    }
                    CLUSTER[id43] occurrences matches {0..1} matches {    -- Document location
                        items cardinality matches {1..*; unordered} matches {
                            ELEMENT[id44] occurrences matches {0..1} matches {    -- Location
                                value matches {
                                    DV_TEXT[id9022] 
                                    DV_URI[id9023] 
                                }
                            }
                            allow_archetype CLUSTER[id45] matches {     -- Copy holder
                                include
                                    archetype_id/value matches {/.*/}
                            }
                        }
                    }
                    allow_archetype CLUSTER[id46] matches {     -- Extension
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

