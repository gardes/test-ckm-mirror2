            ELEMENT[id8] occurrences matches {0..1} matches {    -- Health care facility type
                value matches {
                    DV_TEXT[id9050] 
                }
            }
            ELEMENT[id10] occurrences matches {0..1} matches {    -- Practice setting code
                value matches {
                    DV_TEXT[id9051] 
                }
            }
            ELEMENT[id12] matches {    -- Event code
                value matches {
                    DV_TEXT[id9052] 
                }
            }
            ELEMENT[id11] occurrences matches {0..1} matches {    -- Document_media
                value matches {
                    DV_MULTIMEDIA[id9053] matches {
