definition
    OBSERVATION[id1] matches {    -- Electroretinogram
        data matches {
            HISTORY[id2] matches {
                events cardinality matches {1..*; unordered} matches {
                    POINT_EVENT[id3] occurrences matches {0..1} matches {    -- Point in time
                        data matches {
                            ITEM_TREE[id4] matches {
                                items cardinality matches {0..*; unordered} matches {
                                    CLUSTER[id5] matches {    -- By eye
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[id6] occurrences matches {0..1} matches {    -- Laterality
                                                value matches {
                                                    DV_CODED_TEXT[id9007] matches {
                                                        defining_code matches {[ac9000]}    -- Laterality (synthesised)
                                                    }
                                                }
                                            }
                                            ELEMENT[id9] occurrences matches {0..1} matches {    -- A-wave implicit time
                                                value matches {
                                                    DV_QUANTITY[id9008] matches {
                                                        property matches {[at9001]}    -- Time
                                                        units matches {"millisec"}
                                                    }
                                                }
                                            }
                                            ELEMENT[id10] occurrences matches {0..1} matches {    -- A-wave amplitude
                                                value matches {
                                                    DV_QUANTITY[id9009] matches {
                                                        property matches {[at9002]}    -- Voltage, electrical
                                                        units matches {"mV"}
                                                    }
                                                }
                                            }
                                            ELEMENT[id11] occurrences matches {0..1} matches {    -- B-wave implicit time
                                                value matches {
                                                    DV_QUANTITY[id9010] matches {
                                                        property matches {[at9001]}    -- Time
                                                        units matches {"millisec"}
                                                    }
                                                }
                                            }
                                            ELEMENT[id12] occurrences matches {0..1} matches {    -- B-wave amplitude
                                                value matches {
                                                    DV_QUANTITY[id9011] matches {
                                                        property matches {[at9002]}    -- Voltage, electrical
                                                        units matches {"mV"}
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[id13] occurrences matches {0..1} matches {    -- Electroretinogram response
                                        value matches {
                                            DV_CODED_TEXT[id9012] matches {
                                                defining_code matches {[ac9003]}    -- Electroretinogram response (synthesised)
                                            }
                                        }
                                    }
                                    ELEMENT[id16] occurrences matches {0..1} matches {    -- Adaptation
                                        value matches {
                                            DV_CODED_TEXT[id9013] matches {
                                                defining_code matches {[ac9004]}    -- Adaptation (synthesised)
                                            }
                                        }
                                    }
                                    ELEMENT[id19] occurrences matches {0..1} matches {    -- Stimulation
                                        value matches {
                                            DV_CODED_TEXT[id9014] matches {
                                                defining_code matches {[ac9005]}    -- Stimulation (synthesised)
                                            }
                                        }
                                    }
                                    ELEMENT[id26] occurrences matches {0..1} matches {    -- Assessment
                                        value matches {
                                            DV_CODED_TEXT[id9015] matches {
                                                defining_code matches {[ac9006]}    -- Assessment (synthesised)
                                            }
                                        }
                                    }
                                }
                            }
                        }
                        state matches {
                            ITEM_TREE[id24] matches {
                                items cardinality matches {0..*; unordered} matches {
                                    ELEMENT[id25] matches {    -- Confounding factors
                                        value matches {
                                            DV_TEXT[id9016] 
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id22] matches {
                items cardinality matches {0..*; unordered} matches {
                    allow_archetype CLUSTER[id23] matches {     -- Extension
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

