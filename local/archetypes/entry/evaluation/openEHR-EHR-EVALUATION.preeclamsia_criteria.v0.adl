definition
	EVALUATION[at0000] matches {    -- preeclamsia_criteria
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Urine protein/24 h
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::347]>
								list = <
									["1"] = <
										units = <"g/(24.h)">
										magnitude = <|0.0..50.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Platelet count u/uL
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::129]>
								list = <
									["1"] = <
										units = <"uL">
										magnitude = <|0.0..1000000.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- dipstick >2+
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- doubling of the creatinine concentration (no KD)
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0010] occurrences matches {0..1} matches {    -- Liver transaminases
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {    -- Pulmonary edema
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {    -- Headache
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Visual symptoms
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0009] matches {*}    -- Item tree
		}
	}

