                                                    [{21}, {[at67]}],
                                                    [{31}, {[at68]}],
                                                    [{41}, {[at69]}],
                                                    [{51}, {[at70]}]
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id45] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id47] occurrences matches {0..1} matches {    -- Score version
                        value matches {
                            DV_TEXT[id9019] 
                        }
                    }
                    allow_archetype CLUSTER[id72] matches {     -- Extension
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

