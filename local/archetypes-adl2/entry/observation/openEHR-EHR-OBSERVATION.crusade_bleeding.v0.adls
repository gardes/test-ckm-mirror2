                                                [value, symbol] matches {
                                                    [{0}, {[at28]}],
                                                    [{8}, {[at27]}]
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[id29] occurrences matches {0..1} matches {    -- Signs of heart failure
                                        value matches {
                                            DV_ORDINAL[id9013] matches {
                                                [value, symbol] matches {
                                                    [{0}, {[at30]}],
                                                    [{7}, {[at31]}]
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[id32] occurrences matches {0..1} matches {    -- Prior vascular disease
                                        value matches {
                                            DV_ORDINAL[id9014] matches {
                                                [value, symbol] matches {
                                                    [{0}, {[at33]}],
                                                    [{7}, {[at34]}]
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[id35] occurrences matches {0..1} matches {    -- Diabetes mellitus
                                        value matches {
                                            DV_ORDINAL[id9015] matches {
                                                [value, symbol] matches {
                                                    [{0}, {[at36]}],
                                                    [{6}, {[at37]}]
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[id74] occurrences matches {0..1} matches {    -- Systolic blood pressure
                                        value matches {
                                            DV_ORDINAL[id9016] matches {
                                                [value, symbol] matches {
                                                    [{1}, {[at78]}],
                                                    [{3}, {[at79]}],
                                                    [{5}, {[at77]}],
                                                    [{8}, {[at76]}],
                                                    [{10}, {[at75]}]
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[id65] occurrences matches {0..1} matches {    -- Total score
                                        value matches {
                                            DV_COUNT[id9017] matches {
                                                magnitude matches {|0..91|}
                                            }
                                        }
                                    }
                                    ELEMENT[id64] occurrences matches {0..1} matches {    -- Risk grade
                                        value matches {
                                            DV_ORDINAL[id9018] matches {
                                                [value, symbol] matches {
