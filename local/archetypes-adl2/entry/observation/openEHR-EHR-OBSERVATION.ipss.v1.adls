        ["nb"] = <
            language = <[ISO_639-1::nb]>
            purpose = <"Registrere verdier for prostatasymptomkomponentene og dets summerte verdi for I-PSS.">
            keywords = <"I-PSS", "prostata", "ipss", "bph", "prostatahyperplasi", "vannlatningsbesvær">
