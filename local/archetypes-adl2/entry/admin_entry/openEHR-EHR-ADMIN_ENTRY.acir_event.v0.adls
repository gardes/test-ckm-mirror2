                            DV_BOOLEAN[id9001] matches {
                                value matches {True, False}
                            }
                        }
                    }
                    ELEMENT[id5] occurrences matches {0..1} matches {    -- Medical Contraindication
                        value matches {
                            DV_TEXT[id9002] 
                        }
                    }
                }
            }
        }
    }

