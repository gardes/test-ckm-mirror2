                    }
                }
            }
            ISM_TRANSITION[id15] matches {    -- Informed consent requested
                current_state matches {
                    DV_CODED_TEXT[id9059] matches {
                        defining_code matches {[ac9005]}    -- Informed consent requested (synthesised)
                    }
                }
                careflow_step matches {
                    DV_CODED_TEXT[id9060] matches {
                        defining_code matches {[at15]}    -- Informed consent requested
                    }
                }
            }
            ISM_TRANSITION[id16] matches {    -- Informed consent provided
                current_state matches {
                    DV_CODED_TEXT[id9061] matches {
                        defining_code matches {[at9004]}    -- active
                    }
                }
                careflow_step matches {
                    DV_CODED_TEXT[id9062] matches {
                        defining_code matches {[at16]}    -- Informed consent provided
                    }
                }
            }
            ISM_TRANSITION[id22] matches {    -- Informed consent not obtained
                current_state matches {
                    DV_CODED_TEXT[id9063] matches {
                        defining_code matches {[at9004]}    -- active
                    }
                }
                careflow_step matches {
                    DV_CODED_TEXT[id9064] matches {
                        defining_code matches {[at22]}    -- Informed consent not obtained
                    }
                }
            }
            ISM_TRANSITION[id17] matches {    -- Informed consent refused
                current_state matches {
                    DV_CODED_TEXT[id9065] matches {
                        defining_code matches {[at9006]}    -- aborted
                    }
                }
                careflow_step matches {
                    DV_CODED_TEXT[id9066] matches {
                        defining_code matches {[at17]}    -- Informed consent refused
                    }
                }
            }
            ISM_TRANSITION[id18] matches {    -- Informed consent withdrawn
                current_state matches {
                    DV_CODED_TEXT[id9067] matches {
                        defining_code matches {[at9006]}    -- aborted
                    }
                }
                careflow_step matches {
                    DV_CODED_TEXT[id9068] matches {
                        defining_code matches {[at18]}    -- Informed consent withdrawn
                    }
                }
            }
            ISM_TRANSITION[id23] matches {    -- Completed
                current_state matches {
                    DV_CODED_TEXT[id9069] matches {
                        defining_code matches {[at9007]}    -- complete
                    }
                }
                careflow_step matches {
                    DV_CODED_TEXT[id9070] matches {
                        defining_code matches {[at23]}    -- Completed
                    }
                }
            }
        }
        description matches {
            ITEM_TREE[id2] matches {
                items cardinality matches {1..*; unordered} matches {
                    ELEMENT[id3] occurrences matches {1..*} matches {    -- Procedure/Trial/Activity
                        value matches {
                            DV_TEXT[id9071] 
                        }
                    }
                    ELEMENT[id31] occurrences matches {0..1} matches {    -- Procedure/Trial/Activity description
                        value matches {
