                    }
                    ELEMENT[id9] occurrences matches {0..1} matches {    -- Start date
                        value matches {
                            DV_DATE_TIME[id9075] 
                        }
                    }
                    ELEMENT[id10] occurrences matches {0..1} matches {    -- End date
                        value matches {
                            DV_DATE_TIME[id9076] 
                        }
                    }
                    ELEMENT[id11] matches {    -- Caveat
                        value matches {
                            DV_TEXT[id9077] 
                        }
                    }
                    ELEMENT[id13] occurrences matches {0..1} matches {    -- Evidence link
                        value matches {
                            DV_URI[id9087] 
                        }
                    }
                    ELEMENT[id37] occurrences matches {0..1} matches {    -- Comment
                        value matches {
                            DV_TEXT[id9086] 
                        }
                    }
                    allow_archetype CLUSTER[id38] matches {     -- Evidence details
                        include
                            archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia\.v0\..*/}
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id25] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id26] occurrences matches {0..1} matches {    -- Consent document used
                        value matches {
                            DV_TEXT[id9079] 
                        }
                    }
                    ELEMENT[id27] occurrences matches {0..1} matches {    -- Review date
                        value matches {
                            DV_DATE_TIME[id9080] 
                        }
                    }
                    allow_archetype CLUSTER[id29] matches {     -- Consent requester
                        include
                            archetype_id/value matches {/.*/}
                    }
                    allow_archetype CLUSTER[id30] matches {     -- Consent provider
                        include
                            archetype_id/value matches {/.*/}
                    }
                    CLUSTER[id34] matches {    -- Patient information
                        items cardinality matches {1..*; unordered} matches {
                            ELEMENT[id33] occurrences matches {0..1} matches {    -- Name
                                value matches {
                                    DV_TEXT[id9083] 
                                }
                            }
                            ELEMENT[id36] occurrences matches {0..1} matches {    -- Description
                                value matches {
                                    DV_TEXT[id9084] 
                                }
                            }
                            allow_archetype CLUSTER[id39] matches {     -- Multimedia representation
                                include
                                    archetype_id/value matches {/.*/}
                            }
                        }
                    }
                }
            }
        }
    }

