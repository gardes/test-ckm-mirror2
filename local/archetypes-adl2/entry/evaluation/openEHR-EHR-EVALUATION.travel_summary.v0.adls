                                        }
                                    }
                                    ELEMENT[id12] occurrences matches {0..1} matches {    -- Date of exit
                                        value matches {
                                            DV_DATE_TIME[id9007] 
                                        }
                                    }
                                    ELEMENT[id13] occurrences matches {0..1} matches {    -- Description
                                        value matches {
                                            DV_TEXT[id9008] 
                                        }
                                    }
                                    ELEMENT[id14] occurrences matches {0..1} matches {    -- Comment
                                        value matches {
                                            DV_TEXT[id9009] 
                                        }
                                    }
                                }
                            }
                            ELEMENT[id15] matches {    -- Return transport
                                value matches {
                                    DV_TEXT[id9010] 
                                }
                            }
                            ELEMENT[id16] occurrences matches {0..1} matches {    -- Date of return
                                value matches {
                                    DV_DATE_TIME[id9011] 
                                }
                            }
                            ELEMENT[id17] occurrences matches {0..1} matches {    -- Comment
                                value matches {
                                    DV_TEXT[id9012] 
                                }
                            }
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id18] matches {
                items cardinality matches {0..*; unordered} matches {
                    allow_archetype CLUSTER[id19] matches {     -- Extension
                        include
                            archetype_id/value matches {/.*/}
                    }
                    ELEMENT[id20] occurrences matches {0..1} matches {    -- Last updated
                        value matches {
                            DV_DATE[id9013] 
                        }
                    }
                }
            }
        }
    }

