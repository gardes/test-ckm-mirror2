definition
    EVALUATION[id1] matches {    -- Food and nutrition summary
        data matches {
            ITEM_TREE[id2] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id12] occurrences matches {0..1} matches {    -- Description
                        value matches {
                            DV_TEXT[id9002] 
                        }
                    }
                    ELEMENT[id14] occurrences matches {0..1} matches {    -- Food security status
                        value matches {
                            DV_CODED_TEXT[id9007] matches {
                                defining_code matches {[ac9010]}    -- Food security status (synthesised)
                            }
                        }
                    }
                    ELEMENT[id21] occurrences matches {0..1} matches {    -- Food security description
                        value matches {
                            DV_TEXT[id9008] 
                        }
                    }
                    ELEMENT[id3] occurrences matches {0..1} matches {    -- Nutrition status
                        value matches {
                            DV_TEXT[id9003] 
                        }
                    }
                    ELEMENT[id4] occurrences matches {0..1} matches {    -- Weight status
                        value matches {
                            DV_CODED_TEXT[id9004] matches {
                                defining_code matches {[ac9000]}    -- Weight status (synthesised)
                            }
                        }
                    }
                    ELEMENT[id9] occurrences matches {0..1} matches {    -- Dietary preference
                        value matches {
                            DV_TEXT[id9005] 
                        }
                    }
                    ELEMENT[id10] matches {    -- Dietary constraints
                        value matches {
                            DV_TEXT[id9006] 
                        }
                    }
                    ELEMENT[id11] occurrences matches {0..1} matches {    -- Comment
                        value matches {
                            DV_TEXT[id9009] 
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id13] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id19] occurrences matches {0..1} matches {    -- Last updated
                        value matches {
                            DV_DATE_TIME[id9010] 
                        }
                    }
                    allow_archetype CLUSTER[id20] matches {     -- Extension
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

terminology
    term_definitions = <
        ["de"] = <
            ["ac9010"] = <
                text = <"Status der Ernährungssicherheit (synthesised)">
                description = <"Kategorie, die die Wahrnehmung des Individuums über einen sicheren und zuverlässigen Zugang zu Nahrungsmitteln beschreibt, der in Bezug auf Quantität und Ernährungsqualität angemessen, kulturell akzeptabel, sicher und auf sozial verträgliche Weise erworben ist. (synthesised)">
            >
            ["ac9000"] = <
                text = <"Gewichtsstatus (synthesised)">
                description = <"Beurteilung des Gewichtsstatus des Individuums. (synthesised)">
            >
            ["id21"] = <
                text = <"Beschreibung der Ernährungssicherheit">
                description = <"Beschreibung der Fähigkeit der Person, auf geeignete Nahrung zuzugreifen.">
            >
            ["id20"] = <
                text = <"Erweiterung">
                description = <"Zusätzliche Informationen zur Erfassung lokaler Inhalte oder Anpassung an andere Referenzmodelle/Formalismen.">
                comment = <"Zum Beispiel: Lokaler Informationsbedarf oder zusätzliche Metadaten zur Anpassung an FHIR-Ressourcen oder CIMI-Modelle.">
            >
            ["id19"] = <
                text = <"Letzte Aktualisierung">
                description = <"Das Datum, an dem diese Zusammenfassung das letzte Mal aktualisiert wurde.">
            >
            ["at17"] = <
