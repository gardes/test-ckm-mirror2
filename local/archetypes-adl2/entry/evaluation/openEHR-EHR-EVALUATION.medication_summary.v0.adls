definition
    EVALUATION[id1] matches {    -- Medication summary
        data matches {
            ITEM_TREE[id2] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id3] occurrences matches {0..1} matches {    -- Medication name
                        value matches {
                            DV_TEXT[id9000] 
                        }
                    }
                    ELEMENT[id8] occurrences matches {0..1} matches {    -- Clinical description
                        value matches {
                            DV_TEXT[id9003] 
                        }
                    }
                    ELEMENT[id29] matches {    -- Clinical indication
                        value matches {
                            DV_TEXT[id9019] 
                        }
                    }
                    ELEMENT[id10] occurrences matches {0..1} matches {    -- Onset of use
                        value matches {
                            DV_DATE_TIME[id9004] 
                        }
                    }
                    CLUSTER[id9] matches {    -- Episode
                        items cardinality matches {1..*; unordered} matches {
                            ELEMENT[id12] occurrences matches {0..1} matches {    -- Episode onset
                                value matches {
                                    DV_DATE_TIME[id9006] 
                                }
                            }
                            ELEMENT[id19] matches {    -- Episode indication
                                value matches {
                                    DV_TEXT[id9007] 
                                }
                            }
                            ELEMENT[id21] matches {    -- Therapeutic intent
                                value matches {
                                    DV_TEXT[id9008] 
                                }
                            }
                            ELEMENT[id15] occurrences matches {0..1} matches {    -- Description
                                value matches {
                                    DV_TEXT[id9009] 
                                }
                            }
                            ELEMENT[id17] occurrences matches {0..1} matches {    -- Episode amount
                                value matches {
                                    DV_QUANTITY[id9011] 
                                    DV_TEXT[id9020] 
                                }
                            }
                            allow_archetype CLUSTER[id30] matches {     -- Additional details
                                include
                                    archetype_id/value matches {/openEHR-EHR-CLUSTER\.dosage(-[a-zA-Z0-9_]+)*|openEHR-EHR-CLUSTER\.medication(-[a-zA-Z0-9_]+)*/}
                            }
                            ELEMENT[id13] occurrences matches {0..1} matches {    -- Episode cessation
                                value matches {
                                    DV_DATE_TIME[id9010] 
                                }
                            }
                            ELEMENT[id14] matches {    -- Reason for cessation
                                value matches {
                                    DV_TEXT[id9012] 
                                }
                            }
                            ELEMENT[id23] occurrences matches {0..1} matches {    -- Treatment outcome
                                value matches {
                                    DV_TEXT[id9017] 
                                }
                            }
                        }
                    }
                    ELEMENT[id16] occurrences matches {0..1} matches {    -- Cumulative dose
                        value matches {
                            DV_QUANTITY[id9013] 
                        }
                    }
                    ELEMENT[id11] occurrences matches {0..1} matches {    -- Cessation of use
                        value matches {
                            DV_DATE_TIME[id9014] 
                        }
                    }
                    ELEMENT[id28] occurrences matches {0..1} matches {    -- Cumulative duration
                        value matches {
                            DV_DURATION[id9018] matches {
                                value matches {|>=PT0S|}
                            }
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id6] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id7] occurrences matches {0..1} matches {    -- Last updated
                        value matches {
                            DV_DATE_TIME[id9015] 
                        }
                    }
                    allow_archetype CLUSTER[id20] matches {     -- Extension
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

terminology
    term_definitions = <
        ["de"] = <
            ["id30"] = <
                text = <"Zusätzliche Angaben">
                description = <"Zusätzliche Angaben zur Anwendung des Medikaments innerhalb des Anwendungszeitraums.">
            >
            ["id29"] = <
                text = <"Klinische Indikation">
                description = <"Die allgemeine klinische Indikation für die Anrwendung des Medikaments.">
                comment = <"Die Codierung mit einer externen Terminologie wird nach Möglichkeit bevorzugt.">
            >
            ["id28"] = <
                text = <"Kumulative Dauer">
                description = <"Die Gesamtdauer über alle Anwendungszeiträume.">
                comment = <"Kann manuell berechnet oder über die EHR aus mehreren Quellen abgeleitet werden.">
            >
            ["id23"] = <
                text = <"Behandlungsergebnis">
                description = <"Das beobachtete Ansprechen der Person auf die Behandlung mit diesem Medikament während des Anwendungszeitraums.">
                comment = <"Wenn möglich, wird die Codierung mit einer externen Terminologie bevorzugt. Zum Beispiel: UTI behoben.">
            >
            ["id21"] = <
                text = <"Therapeutische Intention">
                description = <"Die therapeutische Absicht für die Anrwendung des Medikaments in diesem Anwendungszeitraum.">
                comment = <"Codierung mit einer externen Terminologie, soweit möglich. Zum Beispiel: Schmerzlinderung; palliativ oder kurativ; Kurzzeitkurs oder lebenslange Antibiotika.
    	
    
    ">
            >
            ["id20"] = <
                text = <"Erweiterung">
