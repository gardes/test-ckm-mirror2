    details = <
        ["de"] = <
            language = <[ISO_639-1::de]>
            purpose = <"Zur Darstellung der Informationen zu einer Risikoeinschätzung für eine Person, die möglicherweise einem infektiösen Erreger ausgesetzt war.">
            keywords = <"Infektion", "ansteckend", "infektiös", "Ausbruch", "Exposition", "Virus", "Ansteckung", "Pandemie">
            use = <"Verwenden Sie diesen Archetyp, um Informationen zu einer Risikoeinschätzung für eine Person darzustellen, die möglicherweise einem Infektionserreger ausgesetzt war, insbesondere im Zusammenhang mit einem Ausbruch einer Infektionskrankheit.">
            misuse = <"">
        >
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record details about a risk assessment for an individual who may have been exposed to an infectious agent.">
            keywords = <"infection, infectious, outbreak, exposure, virus, contagion, pandemic", ...>
            use = <"Use to record details about a risk assessment for an individual who may have been exposed to an infectious agent, especially in the context of an outbreak of infectious disease.">
            misuse = <"">
            copyright = <"© openEHR Foundation">
        >
        ["it"] = <
            language = <[ISO_639-1::it]>
            purpose = <"*To record details about a risk assessment for an individual who may have been exposed to an infectious agent. (en)">
            keywords = <"*infection, infectious, outbreak, exposure, virus, contagion, pandemic (en)", ...>
            use = <"*Use to record details about a risk assessment for an individual who may have been exposed to an infectious agent, especially in the context of an outbreak of infectious disease. (en)">
            misuse = <"">
        >
    >

definition
    EVALUATION[id1] matches {    -- Infectious exposure investigation
        data matches {
            ITEM_TREE[id2] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id3] occurrences matches {0..1} matches {    -- Infectious agent
                        value matches {
                            DV_TEXT[id9001] 
                        }
                    }
                    ELEMENT[id4] occurrences matches {0..1} matches {    -- Description
                        value matches {
                            DV_TEXT[id9002] 
                        }
                    }
                    CLUSTER[id6] matches {    -- Specific exposure details
                        items cardinality matches {1..*; unordered} matches {
                            ELEMENT[id28] occurrences matches {0..1} matches {    -- Confirmed exposure?
                                value matches {
                                    DV_CODED_TEXT[id9003] matches {
                                        defining_code matches {[ac9000]}    -- Confirmed exposure? (synthesised)
                                    }
                                }
                            }
                            ELEMENT[id25] occurrences matches {0..1} matches {    -- Exposure category
                                value matches {
                                    DV_TEXT[id9004] 
                                }
                            }
                            ELEMENT[id7] occurrences matches {0..1} matches {    -- Description of exposure
                                value matches {
                                    DV_TEXT[id9005] 
                                }
                            }
                            ELEMENT[id8] matches {    -- Date/time of exposure
                                value matches {
                                    DV_DATE_TIME[id9006] 
                                }
                            }
                            ELEMENT[id19] occurrences matches {0..1} matches {    -- Exposed anatomical location
                                value matches {
                                    DV_TEXT[id9007] 
                                }
                            }
                            allow_archetype CLUSTER[id20] matches {     -- Anatomical location details
                                include
                                    archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1\..*/}
                            }
                            ELEMENT[id9] occurrences matches {0..1} matches {    -- Physical location category
                                value matches {
                                    DV_TEXT[id9008] 
                                }
                            }
                            allow_archetype CLUSTER[id27] matches {     -- Structured location details
                                include
                                    archetype_id/value matches {/.*/}
                            }
                            ELEMENT[id10] matches {    -- Specific physical location
                                value matches {
                                    DV_TEXT[id9009] 
                                }
                            }
                            ELEMENT[id21] occurrences matches {0..1} matches {    -- Activity category
                                value matches {
                                    DV_TEXT[id9010] 
                                }
                            }
                            ELEMENT[id22] occurrences matches {0..1} matches {    -- Activity description
                                value matches {
                                    DV_TEXT[id9011] 
                                }
                            }
                            allow_archetype CLUSTER[id23] matches {     -- Personal protective equipment
                                include
                                    archetype_id/value matches {/.*/}
                            }
                            ELEMENT[id12] occurrences matches {0..1} matches {    -- Duration of exposure
                                value matches {
                                    DV_DURATION[id9012] 
                                    DV_TEXT[id9013] 
                                }
                            }
                            ELEMENT[id13] occurrences matches {0..1} matches {    -- Proximity of contact
                                value matches {
                                    DV_TEXT[id9014] 
                                }
                            }
                            allow_archetype CLUSTER[id16] matches {     -- Source individual details
                                include
                                    archetype_id/value matches {/.*/}
                            }
                            ELEMENT[id26] occurrences matches {0..1} matches {    -- Case identifier
                                value matches {
                                    DV_TEXT[id9015] 
                                    DV_IDENTIFIER[id9016] 
                                }
                            }
                            ELEMENT[id5] occurrences matches {0..1} matches {    -- Date of notification/report
                                value matches {
                                    DV_DATE_TIME[id9017] 
                                }
                            }
                            allow_archetype CLUSTER[id17]
                        }
                    }
                    ELEMENT[id31] occurrences matches {0..1} matches {    -- Total number of exposures
                        value matches {
                            DV_COUNT[id9018] 
                        }
                    }
                    ELEMENT[id14] occurrences matches {0..1} matches {    -- Risk stratification
                        value matches {
                            DV_TEXT[id9019] 
                        }
                    }
                    allow_archetype CLUSTER[id18] matches {     -- Exposed individual details
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id32] matches {
                items cardinality matches {0..*; unordered} matches {
                    allow_archetype CLUSTER[id33] matches {     -- Extension
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

