    details = <
        ["de"] = <
            language = <[ISO_639-1::de]>
            purpose = <"Zur Darstellung einer Übersicht über die körperliche Aktivität einer Person.">
            keywords = <"Aktivität", "Training", "körperlich", "Leistungsfähigkeit", "Bewegung", "Kondition", "Gesundheit", "Fitness">
            use = <"Dieser Archetyp sollte zur Darstellung einer zusammenfassenden Aufstellung von typischer körperlicher Aktivität bei Kindern oder Erwachsenen verwendet werden.
    
    Bitte beachten: Es gibt einige offensichtliche Überschneidungen zwischen dem Datenelement \"Status des Aktivitätslevels (PAL)\" in diesem Archetyp und dem Datenelement \"Kategorie der körperlichen Aktivität\" in dem Archetyp OBSERVATION.physical_activity - beide verwenden denselben Wertebereich. Dieser Archetyp sollte zur Aufzeichnung von typischer Aktivität verwendet werden. Wenn die Kategorie zu einem bestimmten Zeitpunkt oder während eines bestimmten Zeitraums dokumentiert wird, sollte der entsprechende Datenpunkt in der Datei OBSERVATION.physical_activity verwendet werden.">
            misuse = <"Nicht zur Darstellung von körperlichen Aktivität zu einem bestimmten Zeitpunkt oder während eines bestimmten Zeitraums verwenden. Für diesen Zweck sollte der Archetyp OBSERVATION.physical_activity genutzt werden.">
        >
        ["sv"] = <
            language = <[ISO_639-1::sv]>
            purpose = <"Att registrera en sammanfattning av en individs typiska fysiska aktivitet.">
            keywords = <"aktivitet", "träning", "fysisk", "kondition">
            use = <"Används för att registrera en sammanfattning av typiska eller vanliga, fysiska aktiviteter hos barn eller vuxna.
    
    Observera: Det finns viss uppenbar överlapp mellan dataelementet \"Fysisk aktivitetsnivå (PAL)\" i denna arketyp och dataelementet \"Fysisk aktivitetskategori\" i OBSERVATION.physical_activity archetype - de använder båda samma värdeuppsättning. Använd den här arketypen när du registrerar den typiska aktiviteten som summativt uttalande, men om avsikt är att registrera kategorin vid en viss tidpunkt eller under en viss tidsperiod, använd sedan motsvarande datapunkt i OBSERVATION.physical_activity.">
            misuse = <"Ska inte användas för att registrera aktuell fysisk aktivitet vid en viss tidpunkt eller under en viss tidsperiod - använd då istället OBSERVATION.physical_activity.">
        >
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record a summary of typical physical activity of an individual.">
            keywords = <"activity", "exercise", "physical", "fitness">
            use = <"Use to record a summary of typical, or usual, physical activity of children or adults.
    
    Please note: There is some apparent overlap between the 'Physical activity level (PAL) status' data element in this archetype and the 'Physical activity category' data element in OBSERVATION.physical_activity archetype - they both use the same value set. Use this archetype when recording the the typical activity as a summative statement, however if the intent is to record the category at a specified point in time or during a specified period of time then use the equivalent data point in the OBSERVATION.physical_activity.">
            misuse = <"Not to be used to record actual physical activity at a specific point in time or during a specified period of time - use OBSERVATION.physical_activity for this purpose.">
            copyright = <"© openEHR Foundation">
        >
        ["nl"] = <
            language = <[ISO_639-1::nl]>
            purpose = <"Om een omschrijving van de typische fysieke activiteit van een individu vast te leggen.">
            keywords = <"Activiteit", "Lichaamsbeweging", "Fysiek", "Fitness">
            use = <"Gebruik om een overzicht vast te leggen van typische of gebruikelijke fysieke activiteit bij kinderen of volwassenen.
    
    Merk op: er is enige overlap tussen het 'Physical activity level (PAL) status' data element in dit archetype en het 'Physical activity category' data element in het OBSERVATION.physical_activity archetype.
    Beiden gebruiken dezelfde waardenset. Gebruik dit archetype voor het vastleggen van de typische activiteit als een opbouwende uitspraak. Als de intentie is om activiteit vast te leggen op een moment of periode, gebruik dan een data point in OBSERVATION.physical_activity.">
            misuse = <"Niet te gebruiken om fysieke activiteit op een bepaald moment of tijdens een bepaalde periode vast te leggen - gebruik hiervoor OBSERVATION.physical_activity.">
        >
    >

definition
    EVALUATION[id1] matches {    -- Physical activity summary
        data matches {
            ITEM_TREE[id2] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id3] occurrences matches {0..1} matches {    -- Physical activity level (PAL) status
                        value matches {
                            DV_CODED_TEXT[id9002] matches {
                                defining_code matches {[ac9000]}    -- Physical activity level (PAL) status (synthesised)
                            }
                            DV_TEXT[id9003] 
                        }
                    }
                    ELEMENT[id4] occurrences matches {0..1} matches {    -- Description
                        value matches {
                            DV_TEXT[id9004] 
                        }
                    }
                    ELEMENT[id6] matches {    -- Barrier
                        value matches {
                            DV_TEXT[id9005] 
                        }
                    }
                    ELEMENT[id7] matches {    -- Enabler
                        value matches {
                            DV_TEXT[id9006] 
                        }
                    }
                    ELEMENT[id9] occurrences matches {0..1} matches {    -- Typical moderate exercise
                        value matches {
                            DV_QUANTITY[id9007] matches {
                                property matches {[at9001]}    -- Frequency
                                [units] matches {
                                    [{"1/d"}],
                                    [{"1/wk"}],
                                    [{"1/mo"}]
                                }
                            }
                        }
                    }
                    ELEMENT[id8] occurrences matches {0..1} matches {    -- Typical vigorous exercise
                        value matches {
                            DV_QUANTITY[id9008] matches {
                                property matches {[at9001]}    -- Frequency
                                [units] matches {
                                    [{"1/d"}],
                                    [{"1/wk"}],
                                    [{"1/mo"}]
                                }
                            }
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id15] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id17] occurrences matches {0..1} matches {    -- Last updated
                        value matches {
                            DV_DATE_TIME[id9009] 
                        }
                    }
                    allow_archetype CLUSTER[id16] matches {     -- Extension
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

