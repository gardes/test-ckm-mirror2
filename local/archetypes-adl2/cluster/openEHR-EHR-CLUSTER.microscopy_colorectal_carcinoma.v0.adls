    details = <
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record detailed findings about colorectal cancer found on microscopic histopathological examination.">
            keywords = <"histopathology", "cancer", "laboratory", "lab", "malignancy", "pathology", "colonic", "rectal", "anal", "GI", "gastrointestinal", "bowel">
            use = <"To record detailed findings about microscopic examination of tissue related to colorecal cancer.
    Use as a component archetype in the context of a suite of archetypes that make up a histopathology report 
    i.e. OBSERVATION.lab_test.histopathology and CLUSTER.macroscopy.colorectal_carcinoma.">
            misuse = <"Not designed to be used within any other archetype other than OBSERVATION.lab_test.histopathology.">
            copyright = <"© openEHR Foundation">
        >
        ["ar-sy"] = <
            language = <[ISO_639-1::ar-sy]>
            purpose = <"لتسجيل الموجودات التفصيلية حول سرطان القولون و المستقيم التي تم اكتشافها عند الفحص الهيستوباثولوجي المجهري">
            keywords = <"الهيستوباثولوجيا", "السرطان", "المعمل/المختبر", "المعمل/المختبر", "الخباثة", "الباثولوجيا", "خاص بالقولون", "خاص بالمستقيم", "خاص بالشرج", "مَعِديّ مِعَويّ", "مَعِديّ مِعَويّ", "الأمعاء">
            use = <"لتسجيل الموجودات التفصيلية حول الفحص المجهري للنسيج المتعلق بسرطان القولون و المستقيم.
    يستخدم هذا النموذج في سياق مجموعة من النماذج التي يتكون منها تقرير الهيستوباثولوجيا, يعني نموذج (ملاحظة.تقرير معملي.الهيستوباثولوجيا) و (عنقود.الفحص العِياني. سرطانة القولون و المستقيم).">
            misuse = <"لم يتم تصميم هذا النموذج ليستخدم في داخل أي نموذج غير (ملاحظة.اختبار معملي.الهيستوباثولوجيا).">
            copyright = <"© openEHR Foundation">
        >
