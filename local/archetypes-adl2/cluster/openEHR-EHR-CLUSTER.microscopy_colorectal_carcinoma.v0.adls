                    }
                    ELEMENT[id325] occurrences matches {0..1} matches {    -- Nodal involvement classification
                        value matches {
                            DV_CODED_TEXT[id9020] matches {
                                defining_code matches {[ac9037]}    -- Nodal involvement classification (synthesised)
                            }
                            DV_TEXT[id9040] 
                        }
                    }
                    ELEMENT[id277] occurrences matches {0..1} matches {    -- Distance from margin
                        value matches {
                            DV_QUANTITY[id9021] matches {
                                property matches {[at9003]}    -- Length
                                magnitude matches {|>=0.0|}
                                units matches {"mm"}
                            }
                        }
                    }
                    ELEMENT[id349] occurrences matches {0..1} matches {    -- Comment
