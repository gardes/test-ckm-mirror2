            ["id159"] = <
                text = <"Presentationsenhet">
                description = <"Presentationsenheten för en enda dos av läkemedlet, för användning med dataelementet 'Strength denominator unit' (Styrkans enhet i nämnare)">
                comment = <"Till exempel: \"tablett\", \"kapsel\", \"puff\", \"inandning\". I de flesta fall, som för tabletter och kapslar, är presentationsenheten identisk med form. För vissa presentationer, såsom inhalatorer, kan form vara \"inhalationspulver\", \"inhalations aerosol\" eller \"inhalator\" medan presentationsenheten är \"inhalation\", \"puff\" eller \"dos\".">
            >
            ["id158"] = <
                text = <"Styrka i nämnare">
                description = <"Värdet av nämnaren för styrkan.">
                comment = <"Till exempel: För en presentationsstyrka på '300 µg/0,3 ml' är nämnarens värde '0,3'. För en presentationsstyrka på '100 mg/tablett' är nämnarens värde '1'.">
            >
            ["id154"] = <
                text = <"Styrka i täljare">
                description = <"Värdet på täljaren för styrkefraktionen.">
                comment = <"Exempel: För en presentationsstyrka på '300 µg/0,3 ml' är styrka täljaren '300'. För en presentationsstyrka på \"100 mg/tablett\" är styrka täljaren \"100\"">
            >
            ["id153"] = <
                text = <"Styrka">
                description = <"Styrkan hos läkemedlet eller läkemedelskomponenten, uttryckt som ett förhållande.">
                comment = <"I vissa fall, som för flytande eller halvfasta läkemedel, är nämnaren av styrka förhållandet en fysisk kvantitet, till exempel 2mg/5ml. I några av dessa fall återspeglar nämnaren också komponentens faktiska volym: 5 ml i föregående exempel. I detta fall skulle \"Styrka (koncentration)\" vara 0,4 mg/ml. I andra fall, där styrkan innefattar en nämnare som inte är en fysisk kvantitet, till exempel 4 mg/tablett, uttrycks nämnaren som ett enhetsvärde '1' med en enhet av '1' och 'tablett' bärs in i elementet \"Presentationsenhet\". Detta arrangemang valdes för att ligga i linje med den metod som antagits av ISO IDMP-standarden för läkemedelskataloger.">
            >
            ["id152"] = <
                text = <"Tillverkare">
                description = <"Tillverkaren av läkemedlet eller läkemedelskomponenten.">
                comment = <"Till exempel: 'Abbott'.">
            >
            ["id151"] = <
                text = <"Batch ID">
                description = <"Identifieraren som tillverkaren tilldelade batchen med, under produktionen.">
            >
            ["id149"] = <
                text = <"Alternativ mängd">
                description = <"Värdet av en motsvarande representation av mängden läkemedel eller läkemedelskomponent.">
                comment = <"Enheten för den alternativa mängden registreras i dataelementet 'Alternate amount unit' (Alternativ mängd enhet). 
    Till exempel: för ett läkemedel med en styrka av '5 mg/ml' där mängden är '1 ml' skulle motsvarande mängd vara 5 mg och värdet som registreras i detta dataelement skulle vara '5'.">
            >
            ["at147"] = <
                text = <"Ingrediens">
                description = <"Läkemedlet eller läkemedelskomponenten är en enskild ingrediens i läkemedlet. Denna term används när arketypen är kapslad i en förälder-instans av sig själv för att beskriva de enskilda ingredienserna i ett läkemedel.">
            >
            ["at146"] = <
                text = <"En-ingrediens produkt">
                description = <"Läkemedlet eller läkemedelskomponenten är en tillverkad produkt som innehåller en enda aktiv ingrediens.">
            >
            ["at145"] = <
                text = <"Produkt med flera ingredienser">
                description = <"Läkemedels- eller läkemedelskomponenten består av ett antal aktiva ingredienser som i förväg kombineras till en enda form såsom en tablett eller pulver av tillverkaren, till exempel Paracetamol / kodein.">
            >
            ["at144"] = <
                text = <"Ad-hoc blandning">
                description = <"Läkemedels- eller läkemedelskomponenten består av en blandning av ingredienser som anges i beställningen. Dessa bereds vanligtvis av apotek eller avdelningspersonal för att passa enskilda patienter">
            >
            ["id143"] = <
                text = <"Kategori">
                description = <"Kategori av läkemedels- eller läkemedelskomponenten, med avseende på tillverkning eller beredning, och antalet ingredienser.">
                comment = <"Till exempel: 'Paracetamol/codeine' är en produkt med flera ingredienser, medan 'Morfin 60 mg + Haloperidol 2 mg + Midazolam 5 mg' är en ad-hoc-blandning vars sammansättning är fullständigt specificerad i beställningen.">
            >
            ["id142"] = <
                text = <"Strukturerade detaljer">
                description = <"Ytterligare information om läkemedlet eller läkemedelskomponenten.">
                comment = <"Till exempel: detaljerad information om läkemedelsklass eller avsedda administreringsvägar, eller ytterligare hållbarhetsinformation.
    ">
            >
            ["id140"] = <
                text = <"Mängd">
                description = <"Värdet av mängden läkemedel eller av en läkemedelskomponent.">
                comment = <"Till exempel: '1', '1,5', '1000'.
    Obs! Den tillhörande enheten för denna mängd registreras med hjälp av elementet 'Amount unit' (Mängd enhet).">
            >
            ["id139"] = <
                text = <"Beståndsdelar">
                description = <"Detaljer om en ingrediens eller produkt som används för att göra en blandad förpackning, beredning eller infusion.">
                comment = <"Denna öppning är avsedd att användas för att lägga till detaljer om beståndsdelar i läkemedlet eller läkemedelskomponenten, med hjälp av nästlade instanser av denna arketyp. Detta krävs normalt inte annat än när en blandning beskrivs">
            >
            ["id134"] = <
                text = <"Beskrivning">
                description = <"Utförlig beskrivning av läkemedlet eller läkemedelskomponenten där det inte är möjligt att beskriva detta fullständigt med hjälp av de strukturerade dataelementen.">
            >
            ["id133"] = <
                text = <"Namn">
                description = <"Namnet på läkemedlet eller läkemedelskomponenten.">
                comment = <"Till exempel: 'Zinacef 750 mg pulver' eller 'cefuroxim'. Detta dataelement bör om möjligt kodas, med exempelvis RxNorm, DM + D, Australian Medicines Terminology eller FEST. Användningen av detta dataelement kommer att variera beroende på användningssammanhang. Detta dataelement kan utelämnas om läkemedlets namn registreras i föräldrarnas INSTRUKTION eller ÅTGÄRD, och denna arketyp endast används för att registrera att formen måste vara eller var \"flytande\".">
            >
            ["id128"] = <
                text = <"Roll">
                description = <"Läkemedlets eller läkemedelskomponentens roll i en blandning.">
            >
            ["id116"] = <
                text = <"Styrka (koncentration)">
                description = <"Styrkan hos läkemedlet eller läkemedelskomponenten, som koncentration.">
                comment = <"Detta dataelement används för flytande eller halvfasta läkemedel, eller läkemedel som är avsedda att spädas i en vätska före administrering. 
    Till exempel: '10 mg/ml ', '20 mg/g', '5%', '10, 000 SQ-U/ml '.">
            >
            ["at85"] = <
                text = <"Hjälpämne">
                description = <"Beståndsdel som är trögt i förhållande till läkemedlets avsedda aktivitet.">
            >
            ["at84"] = <
                text = <"Adjuvant">
                description = <"Beståndsdel vars primära funktion är att modifiera aktiviteten hos en aktiv beståndsdel. En adjuvant beståndsdel kan i sig eller inte vara terapeutiskt aktiv.">
            >
            ["at81"] = <
                text = <"Terapeutisk">
                description = <"Beståndsdel som ensam eller i kombination med en eller flera andra ingredienser anses uppfylla läkemedlets avsedda aktivitet.">
            >
            ["id72"] = <
                text = <"Form">
                description = <"Formen eller presentationen av ett läkemedel eller läkemedelskomponent.">
                comment = <"Till exempel: \"tablett\", \"kapsel\", \"infusionsvätska\" eller \"inhalationspulver\". 
    Kodning av formen med en terminologi är att föredra, där det är möjligt. Läkemedelskataloger kan skilja mellan administrerbar form 'lösning för injektion' och produktform 'pulver för injektionsvätska, lösning'. Den dokumenterade formen beror på det exakta sammanhanget för användning. Administrerbar form kommer sannolikt att användas i de flesta fall.">
            >
            ["id9"] = <
                text = <"Mängd enhet">
                description = <"Enheten för mängden läkemedel eller läkemedelskomponent.">
                comment = <"Till exempel: 'mg', 'ml', 'IE'.
    Anmärkning: Mängdens värde registreras med dataelementet 'Amount' (Mängd).">
            >
            ["id8"] = <
                text = <"Alternativ mängd enhet">
                description = <"Enheten för mängden läkemedel eller läkemedelskomponent.">
                comment = <"Värdet på den alternativa mängden registreras i elementet 'Alternativ mängd' 
    Till exempel: för ett läkemedel med en styrka av '5 mg/ml' där mängden är '1 ml' skulle motsvarande mängd vara 5 mg och värdet som registreras i detta dataelement skulle vara 'mg'.">
            >
            ["id6"] = <
                text = <"Styrkans enhet i nämnaren">
                description = <"Nämnarens enheten för styrka.">
                comment = <"Styrka i nämnaren registreras vanligtvis med hjälp av massa eller volymenheter. Till exempel: 'g', 'ml'. För en presentationsstyrka på '300 µg/0,3 ml' är styrkans nämnares enhet 'ml'. För en presentationsstyrka på \"100 mg/tablett\" är styrkans nämnares enhet \"1\". I detta exempel används elementet 'Unit of presentation' (Presentationsenhet) för att registrera presentationsenheten för läkemedlet, 'tablett'.">
            >
            ["id5"] = <
                text = <"Styrkans enhet i täljaren">
                description = <"Enheten för täljaren styrkefraktion.">
                comment = <"Styrkans täljare dokumenteras vanligtvis med massa, volym eller godtyckliga enheter. Till exempel: 'mg', 'ml', 'IE'. För en presentationsstyrka på '300 µg/0,3 ml' är räknarenheten för styrka 'µg'. För en presentationsstyrka på '100 mg/tablett' är täljarens enhet på styrka 'mg'.">
            >
            ["id4"] = <
                text = <"Hållbarhetsdatum">
                description = <"Utgångsdatum och/eller tidpunkt för läkemedels- eller läkemedelskomponenten, enligt tillverkarens eller individens beredning av blandningen.">
                comment = <"Annan form av utgångsdatum, till exempel tid från produktion eller beroende på lagringsmiljö, kan infogas med en specifikt CLUSTER i substansdetaljer eller läggas till som en del av beskrivningen.
    Till exempel: '2017-05-23'.">
