            keywords = <"السرطان الميلانيني", "المعمل/المختبر", "الخباثة", "الجلد/البشرة", "طب الجلد", "الهيستولوجيا", "الهيستوباثولوجيا", "الباثولوجيا/المَرَضية", "السرطان", "الباثولوجيا الجلدية", "المعمل/المختبر">
